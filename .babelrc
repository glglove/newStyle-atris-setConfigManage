{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        },
        "development": {
          "plugins": ["dynamic-import-node"]  //babel-plugin-dynamic-import-node插件是使import() 替换成 require 编译
        },
        "production": {
          "plugins": ["dynamic-import-node"] //babel-plugin-dynamic-import-node插件是使import() 替换成 require 编译
        }        
      }
    ],
    "stage-2"
  ],
  "plugins": ["transform-runtime"],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul","transform-vue-jsx"]
    }
  }
}

